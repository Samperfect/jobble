      <!-- Sidebar Backdrop -->
      <div class="sidebar-backdrop"></div>

      <!-- User Sidebar -->

      <!-- End User Sidebar -->

      <!-- Post A New Job -->
      <section class="user-dashboard">
        <div class="dashboard-outer">
          <div class="upper-title-box">
            <%- include('./partials/flash') %>

            <h3>Welcome, <%= user.fname %></h3>
            <!-- <div class="text">Ready to jump back in?</div> -->
          </div>

          <div class="row">
            <div class="col-lg-6">
              <!-- Ls widget -->
              <div class="ls-widget">
                <div class="tabs-box">
                  <div class="widget-title">
                    <h4>Update Profile</h4>
                  </div>

                  <div class="widget-content">
                    <form
                      class="default-form"
                      action="/user/update"
                      method="POST"
                    >
                      <div class="row">
                        <!-- Input -->
                        <div class="form-group col-lg-12 col-md-12">
                          <label>First Name</label>
                          <input
                            value="<%= user.fname %>"
                            required
                            type="text"
                            name="fname"
                          />
                          <% if (typeof errors !== 'undefined') { %>
                          <small class="text-danger"><%= errors.gen %></small>

                          <% } %>
                        </div>
                        <div class="form-group col-lg-12 col-md-12">
                          <label>Last Name</label>
                          <input
                            value="<%= user.lname %>"
                            required
                            type="text"
                            name="lname"
                          />
                          <% if (typeof errors !== 'undefined') { %>
                          <small class="text-danger"><%= errors.gen %></small>

                          <% } %>
                        </div>
                        <div class="form-group col-lg-12 col-md-12">
                          <label>Email</label>
                          <input
                            value="<%= user.email %>"
                            required
                            type="email"
                            name="email"
                          />
                          <% if (typeof errors !== 'undefined') { %>
                          <small class="text-danger"><%= errors.email %></small>
                          <br />
                          <small class="text-danger"><%= errors.gen %></small>
                          <% } else { %> <% } %>
                        </div>
                        <div class="form-group col-lg-12 col-md-12">
                          <label>Address</label>
                          <input
                            value="<%= user.address %>"
                            required
                            type="text"
                            name="address"
                          />
                          <% if (typeof errors !== 'undefined') { %>
                          <small class="text-danger"><%= errors.gen %></small>

                          <% } %>
                        </div>
                        <div class="form-group col-lg-12 col-md-12">
                          <label>Phone</label>
                          <input
                            value="<%= user.phone %>"
                            required
                            type="text"
                            name="phone"
                          />
                          <% if (typeof errors !== 'undefined') { %>
                          <small class="text-danger"><%= errors.gen %></small>

                          <% } %>
                        </div>

                        <!-- Input -->
                        <div class="form-group col-lg-12 col-md-12 text-right">
                          <input
                            type="submit"
                            value="Update Profile"
                            class="theme-btn btn-style-one"
                          />
                        </div>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
            </div>
            <div class="col-lg-6">
              <!-- Ls widget -->
              <div class="ls-widget">
                <div class="tabs-box">
                  <div class="widget-title">
                    <h4>Make A Service Request</h4>
                  </div>

                  <div class="widget-content">
                    <form
                      class="default-form"
                      action="/user/request"
                      method="POST"
                    >
                      <div class="row">
                        <!-- Input -->
                        <div class="form-group col-lg-12 col-md-12">
                          <label>Job Address</label>
                          <input
                            type="text"
                            name="title"
                            required
                            placeholder="Please enter address and how to access property"
                            value=""
                          />
                        </div>

                        <!-- About Company -->
                        <div class="form-group col-lg-12 col-md-12">
                          <label>Job Description</label>
                          <textarea
                            required
                            name="desc"
                            placeholder="Please enter job description"
                          ></textarea>
                        </div>

                        <!-- Input -->
                        <div class="form-group col-lg-12 col-md-12 text-right">
                          <button class="theme-btn btn-style-one">
                            Request Service
                          </button>
                        </div>
                      </div>
                    </form>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </section>
      <!-- Post A New Job End -->

      <!-- Copyright -->
      <div class="copyright-text">
        <p>© 2021 Kris. All Right Reserved.</p>
      </div>
    </div>
